		{
			CopyString (configuracao, 0, resposta, 9, 3);
			Scan(configuracao, "%s>%s", configuracao);//elimina tab ou espaco apos a string
			if(strcmp(configuracao, "ON") == 0)
				em_debug = ON;
			else
